{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "ts-node ./src/main.ts",
    "start:watch": "nodemon ./src/main.ts",
    "build:prod": "tsc --build tsconfig.build.json",
    "start:prod:local": "run-s build:prod copy:env-file-to-built start:built",
    "start:prod": "run-s build:prod start:built",
    "start:built": "node dist/src/main.js",
    "copy:env-file-to-built": "shx cp -r ./src/.env ./dist/src/",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:staged": "jest --config=jest.json --changedSince=main",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "eslint": "eslint",
    "copy:env-file-from-example": "shx cp ./src/.env.example ./src/.env"
  },
  "dependencies": {
    "@nestjs/common": "~8.2.6",
    "@nestjs/core": "~8.2.6",
    "@nestjs/jwt": "~8.0.0",
    "@nestjs/microservices": "~8.2.6",
    "@nestjs/passport": "~8.1.0",
    "@nestjs/platform-express": "~8.2.6",
    "@nestjs/serve-static": "~2.2.2",
    "@nestjs/testing": "~8.2.6",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "~8.0.3",
    "@nestjs/websockets": "~8.2.6",
    "argon2": "~0.28.3",
    "aws-sdk": "^2.1066.0",
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.13.2",
    "compression": "^1.7.4",
    "dotenv": "~14.3.2",
    "express": "~4.17.2",
    "express-slow-down": "^1.4.0",
    "jsonwebtoken": "~8.5.1",
    "lodash": "^4.17.21",
    "nestjs-pino": "^2.5.0",
    "passport": "~0.5.2",
    "passport-jwt": "~4.0.0",
    "passport-local": "~1.0.0",
    "pg": "~8.7.1",
    "pino-http": "^6.6.0",
    "pino-pretty": "^7.5.1",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "rxjs": "~7.5.2",
    "slug": "~5.2.0",
    "typeorm": "~0.2.41",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "~8.2.0",
    "@nestjs/schematics": "~8.0.5",
    "@nestjs/testing": "~8.2.6",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1",
    "@types/compression": "^1",
    "@types/eslint": "^8",
    "@types/express": "~4.17.13",
    "@types/express-slow-down": "^1",
    "@types/jest": "~27.4.0",
    "@types/lodash": "~4.14.178",
    "@types/node": "~16.11.7",
    "@types/supertest": "~2.0.11",
    "@types/validator": "^13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "~26.0.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "jest": "~27.4.7",
    "nodemon": "~2.0.15",
    "source-map-support": "~0.5.21",
    "supertest": "~6.2.2",
    "ts-jest": "~27.1.3",
    "ts-loader": "~9.2.6",
    "ts-node": "~10.4.0",
    "tsconfig-paths": "~3.12.0",
    "typescript": "~4.5.5"
  }
}
